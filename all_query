Test case 13: AT Command
{
   "query": {
    "bool": {
      "filter": [
        {
          "match_phrase": {
            "event.code": "106"
          }
        },
        {
          "match_phrase_prefix": {
            "winlog.event_data.TaskName": "\\At"
          }
        },
        {
          "range": {
            "@timestamp": {
              "gte": "2020-06-23T17:00:00.000Z",
              "lte": "2020-06-24T16:59:59.999Z",
              "format": "strict_date_optional_time"
            }
          }
        }
      ]
    }
  }
}

