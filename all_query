Test case 9: WCE - Password and Hash Dump
{
   "query": {
    "bool": {
      "must": [],
      "filter": [
        {
          "match_phrase": {
            "event.code": "11"
          }
        },
        {
          "match_phrase": {
            "file.path": "wceaux.dll"
          }
        },
        {
          "range": {
            "@timestamp": {
              "gte": "2020-06-24T03:58:31.880Z",
              "lte": "2020-06-24T04:13:31.880Z",
              "format": "strict_date_optional_time"
            }
          }
        }
      ]
    }
  }
}

