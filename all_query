Test case 17: ntdsutil
{
  "query": {
    "bool": {
      "should": [
        {
          "bool": {
            "filter": [
              {
                "match_phrase": {
                "event.code": "8222"
                }
              },
              {
                "range": {
                  "@timestamp": {
                    "gte": "2020-06-23T17:00:00.000Z",
                    "lte": "2020-06-24T16:59:59.999Z",
                    "format": "strict_date_optional_time"
                  }
                }
              }
            ]
          }
        },
        {
          "bool": {
            "filter": [
            {
              "match_phrase": {
                "event.code": "4663"
              }
            },
            {
              "match_phrase": {
                "winlog.event_data.ProcessName": "C:\\Windows\\System32\\ntdsutil.exe"
              }
            },
            {
              "match_phrase": {
                "winlog.event_data.ObjectName": "\\Windows\\NTDS"
              }
            },
              {
                "range": {
                  "@timestamp": {
                    "gte": "2020-06-23T17:00:00.000Z",
                    "lte": "2020-06-24T16:59:59.999Z",
                    "format": "strict_date_optional_time"
                  }
                }
              }
            ]
          }
        }
      ],
      "minimum_should_match": 1
    }
  }
}
