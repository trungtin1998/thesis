Test case 10: WCE Remote Login
{
  "query": {
    "bool": {
      "filter": [
        {
          "match_phrase": {
            "event.code": "7045"
          }
        },
        {
          "match_phrase": {
            "winlog.event_data.ServiceName": "WCESERVICE"
          }
        },
        {
          "range": {
            "@timestamp": {
              "gte": "2020-06-24T08:35:06.332Z",
              "lte": "2020-06-24T08:50:06.332Z",
              "format": "strict_date_optional_time"
            }
          }
        }
      ]
    }
  }
}

